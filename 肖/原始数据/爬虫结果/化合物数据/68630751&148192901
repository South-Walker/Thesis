    var str407='1/1/3/1/;1/1/3/2/;1/1/3/3/;1/1/3/4/;1/1/3/5/;1/1/3/6/;1/1/3/7/;1/1/3/8/;1/1/3/9/;1/1/3/10/;1/1/3/11/;1/1/3/12/;1/1/3/13/;1/1/3/14/;1/1/3/15/;1/1/3/16/;1/1/3/17/;1/1/3/18/;1/1/3/19/;1/1/3/20/;1/1/3/21/;1/1/3/22/;1/1/3/23/;1/1/3/24/;1/1/3/25/;1/1/3/26/;1/1/3/27/;1/1/3/28/;1/1/3/29/;1/1/3/30/;1/1/3/31/;1/1/3/32/;1/1/3/33/;1/1/3/34/;1/1/3/35/;1/1/3/36/;1/1/3/37/;1/1/3/38/;1/1/3/39/;1/1/3/40/;1/1/3/41/;1/1/3/42/;1/1/3/43/;1/1/3/44/;1/1/3/45/;1/1/3/46/;1/1/3/47/;1/1/3/48/;1/1/3/49/;1/1/3/50/;1/1/3/51/;1/1/3/52/;1/1/3/53/;1/1/3/54/;1/1/3/55/;1/1/3/56/;1/1/3/57/;1/1/3/58/;1/1/3/59/;1/1/3/60/;1/1/3/61/;1/1/3/62/;1/1/3/63/;1/1/3/64/;1/1/3/65/;1/1/3/66/;1/1/3/67/;1/1/3/68/;1/1/3/69/;1/1/3/70/;1/1/3/71/;1/1/3/72/;1/1/3/73/;1/1/3/74/;1/1/3/75/;1/1/3/76/;1/1/3/77/;1/1/3/78/;1/1/3/79/;1/1/3/80/;1/1/3/81/;1/1/3/82/;1/1/3/83/;1/1/3/84/;1/1/3/85/;1/1/3/86/;1/1/3/87/;1/1/3/88/;1/1/3/89/;1/2/3/90/;1/2/3/91/;1/2/3/92/;1/2/3/93/;1/1/4/1/;1/1/4/4/;1/1/4/5/;1/1/4/6/;1/1/4/7/;1/1/4/8/;1/1/4/10/;1/1/4/11/;1/1/4/12/;1/1/4/13/;1/1/4/15/;1/1/4/16/;1/1/4/17/;1/1/4/18/;1/1/4/19/;1/1/4/20/;1/1/4/21/;1/1/4/22/;1/1/4/23/;1/1/4/24/;1/1/4/25/;1/1/4/26/;1/1/4/27/;1/1/4/28/;1/1/4/29/;1/1/4/30/;1/1/4/31/;1/1/4/32/;1/1/4/33/;1/1/4/34/;1/1/4/35/;1/1/4/36/;1/1/4/37/;1/1/4/38/;1/1/4/39/;1/1/4/40/;1/1/4/42/;1/1/4/43/;1/1/4/44/;1/1/4/45/;1/1/4/46/;1/1/4/48/;1/1/4/49/;1/1/4/50/;1/1/4/51/;1/1/4/52/;1/1/4/53/;1/1/4/54/;1/1/4/55/;1/1/4/56/;1/1/4/58/;1/1/4/59/;1/1/4/60/;1/1/4/61/;1/1/4/62/;1/1/4/63/;1/1/4/64/;1/1/4/65/;1/1/4/66/;1/1/4/67/;1/1/4/68/;1/1/4/69/;1/1/4/70/;1/1/4/71/;1/1/4/72/;1/1/4/73/;1/1/4/74/;1/1/4/75/;1/1/4/76/;1/1/4/77/;1/1/4/78/;1/1/4/79/;1/1/4/80/;1/1/4/81/;1/1/4/82/;1/1/4/83/;1/1/4/84/;1/1/4/85/;1/1/4/86/;1/1/4/88/;1/1/4/89/;1/1/4/90/;1/1/4/91/;1/1/4/92/;1/1/4/93/;1/1/4/94/;1/1/4/9/;1/1/4/14/;1/1/4/41/;1/1/4/47/;1/1/4/57/;1/1/4/87/;1/1/4/2/;1/1/4/3/;1/2/4/95/;1/2/4/96/;1/2/4/97/;';
    var str502='ffffffff/1/;ffff0000/1/;';
    var str503='0/0/0/0/0/;';
    var str601='';
    var Nstr='C/C/C/C/C/C/C/N/C/C/N/C/C/C/N/O/C/O/C/C/N/C/C/C/C/N/O/O/C/N/C/O/C/N/C/C/N/C/C/C/C/C/N/O/O/C/C/C/C/C/C/C/C/N/O/O/O/C/C/C/C/C/C/C/N/O/C/C/C/C/C/C/N/O/C/C/C/C/C/C/C/N/N/O/C/C/N/N/C/C/C/O/O/';
    var shuziMode='0';
    
    
    var Narray = [],
	arrayDI401 = [],
	arrayDI402 = [];
	arrayDI404 = [];
    var array401 = [],DI401 = [],
    	array402 = [],
    	array403 = [],
    	array502 = [],
    	array503 = [],
    	array406 = [],
    	array407 = [],
    	array601 = []
    	;
    
    
    var arrayDI401 = str401.split(';');
    var Narray = Nstr.split('/');
    //document.write(Strarray.length + "<br>");
    var t1,t2,x1,y1;
    var nd1,nd2;
    var minx = 0, maxx=0, miny=0, maxy = 0;
    
    //------------------------------------------------------------
    //var fntdx=ctx.measureText('text');    
    //ctx.fillStyle="red";
    for (var k=0 ; k<arrayDI401.length-1; k++)
    {
	var temp=arrayDI401[k];
	var temparr=temp.split('/');	//将数据再次拆开,拆出结点坐标存入DI401，以供后面矫正图形
	t1=parseFloat(temparr[0]) ;
	t2=parseFloat(temparr[1]) ;
	nd1=parseFloat(temparr[2]) ;
	nd2=parseFloat(temparr[3]) ;
	x1=parseInt(t1)+0.5 ;
    	y1=parseInt(t2) +0.5;	
	//ctx.fillText(Narray[k],x1-5,y1-5);
	//将数据冲入数组sin
	//sin.push([t1,-0.01]); 
	//var strxy=x1.toString() &"/" &y1.toString();
	//array401.push(strxy);
	array401.push([x1,y1,nd1,nd2]); 
	//document.write("x1:" + x1 + ",y1:" + y1 + ";<br>");
	//document.write(t1);
	//sin.push(null); 
	if (k==0) 
	{
		minx=parseInt(t1);
		maxx=parseInt(t1);
		miny=parseInt(t2);
		maxy=parseInt(t2);
	}
	//document.write("x1:" + x1 + ",y1:" + y1 + ";t1:" + t1 + ",t2:" + t2 +";<br>");
	if (minx>t1)
		minx = parseInt(t1) - 1;
	if (maxx<t1)
		maxx = parseInt(t1) + 1;
	if (miny>t2)
		miny = parseInt(t2) - 1;
	if (maxy<t2)
		maxy = parseInt(t2) + 1;
	//if (j==0) then
	//	ctx.moveTo(t1, t2);  
	//else
	//	ctx.lineTo(t1, t2);
    } 
    
    //------------------------------------------------------------    
    //document.write("minx:" + minx + ",miny:" + miny + ";maxx:" + maxx + ",maxy:" + maxy +";<br>");
    //minx:92,miny:140;maxx:139,maxy:195;
    //图形居中，由于这个坐标和canvas的坐标不一致，故需要对照调整
    //canvas实际坐标只有【300,150】，
    //如果被设置成【600，300】后，数据导入后会放大1倍显示，因此在图形居中的过程中要考虑画布的放大效应
    //计算居中需要的偏移值
    var midx = (minx + maxx) / 2;
    var midy = (miny + maxy) / 2;
    //var offsetx = midx -canvas_x/2;
    //var offsety = midy - canvas_y/2;
    var offsetx = 0;
    var offsety = 0;
    //-----------------------------------------
    //从503中截取出色彩
    array503=str503.split(';');
    
    //-----------------------------------------
    //从406中截取出线段类型和色彩型号
    array406=str406.split(';');
    for (var f=0; f<array406.length-1;f++)
    {
    	//document.write ("array406[f]:" + array406[f] +  ";<br>");
    }
    //
    //--------------------------------------------------
    //先画出图形
    //-------------------------------------------------
    //document.write("midx:" + midx + ",midy:" + midy + ";offsetx:" + offsetx + ",offsety:" + offsety +";<br>");
    //midx:115.5,midy:167.5;offsetx:40.5,offsety:17.5;
    var arrayDI403 = str403.split(';');
    var lenar3=arrayDI403.length;
    //document.write("arrayDI403.length:" + arrayDI403.length +  ";<br>");
    //ctx.beginPath();  
    var lxcrd=[],
	lycrd=[],
	elxcrd=[],
	elycrd=[],
	specfeat3=[],
	RefItem3=[];
    for (var f=0; f<arrayDI403.length-1;f++)
    {
    	var temp403=arrayDI403[f];
	var temparr=temp403.split('/');	//将数据再次拆开,拆出线段坐标，准备画线
	lxcrd[f]=parseInt(temparr[0]) ;
	lycrd[f]=parseInt(temparr[1]) ;
	elxcrd[f]=parseInt(temparr[2]) ;
	elycrd[f]=parseInt(temparr[3]) ;
	specfeat3[f]=parseInt(temparr[4]) ;
	RefItem3[f] =parseInt(temparr[5]) ;
	var x1=lxcrd[f] - offsetx;
    	var y1=lycrd[f] - offsety;
    	var x2=elxcrd[f] - offsetx;
    	var y2=elycrd[f] - offsety;
    	//从503中取出显示属性
    	var disp=array406[ specfeat3[f]-1 ];
    	//分析显示属性
    	//document.write ("specfeat3[f]:" + specfeat3[f] +  ";<br>");
    	//document.write ("disp:" + disp +  ";<br>");
    	var dsparr=disp.split('/');	//  0/1/2/;
    	var xianduan=dsparr[1];		//  ffff0000/1/;
    	var colo;			//  255/0/0/0/0/;
    	if (dsparr[2]>1)
    		colo="red";
    	else
    		colo="black";
    	if (xianduan<2)
    	{	//画实线
		ctx.beginPath();
		ctx.moveTo(x1, y1);  
		ctx.lineTo(x2, y2);
		ctx.closePath(); 
		ctx.strokeStyle=colo;
		ctx.stroke();
		//ctx.fillText(x1,x1,y1+widtey+3.5);
		//ctx.fill();
	}
	else
	{
		//画虚线
		//var img = new Image();   
		//var img = document.createElement('img');
        	//img.src = "dash.JPG";   
        	//img.onload = loaded(); 
        	//var pattern = ctx.createPattern(img, "repeat");   
		
		//ctx.beginPath();
		//ctx.moveTo(x1, y1);  
		//ctx.lineTo(x2, y2);
		//ctx.strokeStyle=pattern;
		//ctx.closePath(); 
		//ctx.strokeStyle="#ff0000";
		//ctx.stroke();
		//document.write("x1:" + x1 + ",y1:" + y1 + ";x2:" + x2 + ",y2:" + y2 +";specfeat3:" + specfeat3[f]+"; " + f +"<br>");
		ctx.strokeStyle=colo;
		drawDashedLine(ctx,x1,y1,x2,y2);
	}
	
    }
    //ctx.closePath(); 
    ctx.strokeStyle = "black";
    ctx.font="normal 100 11px Arial";
    ctx.lineWidth = 1;
    ctx.stroke();

   //------------------------------------------------------------------------
   //strDI404内容非空则表示有楔形键
   //str404:146/173,162/142,154/139,1/2/16;185/196,201/217,196/220,1/2/18;
   var arrayDI404 = str404.split(';');
   var pxcrd=[],pycrd=[],temp4a=[],temp4c=[];
   for (var p=0; p<arrayDI404.length-1; p++)
   {
   	var temp404=arrayDI404[p];
   	var temp4a=temp404.split(',');
   	ctx.beginPath();   	    	
   	ctx.fillStyle = "black";
   	for (var p4=0; p4<temp4a.length; p4++)
   	{ 
   	    if (p4==temp4a.length-1)
   	    {
   		var temp4fill=temp4a[p4];		//前面的是坐标,最后一个是显示参数  	   		
   	    }	
   	    else
   	    {   	    	
   	    	var temp4crd = temp4a[p4];
   	    	var temp4c=temp4crd.split('/');
   	    	var x2=temp4c[0];
   	    	var y2=temp4c[1];
   	    	x2=x2 -  offsetx;
   	    	y2=y2 - offsety;
   	    	if (p4==0)
   	    		ctx.moveTo(x2, y2);
   	    	else
   	    		ctx.lineTo(x2, y2);		
   	    }
   	}
   	ctx.closePath(); 
	ctx.fill();
   }
   
   //---------------------------------------------------------
   //从601中读取结点顺序号，用于原子序号显示
   array601=str601.split('/');
    
    //------------------------------------------------------------
    //开始找到字符串坐标，并存入DI402
    var arrayDI402 = str402.split(';');
    //document.write(Strarray.length + "<br>");    
    //var fntdx=ctx.measureText('text');    
    //ctx.fillStyle="red";   
    var lenar2=arrayDI402.length; 
    ctx.fillStyle = "white";    
    ctx.lineWidth = 1;
    
    var sxcrd=[],
	sycrd=[],
	esxcrd=[],
	esycrd=[],
	specfeat2=[];
    for (var k=0 ; k<arrayDI402.length-1; k++)
    {
	var temp402=arrayDI402[k];
	var temparr=temp402.split('/');	//将数据再次拆开,拆出字符串坐标，准备填充
	sxcrd[k]=parseInt(temparr[0]) ;
	sycrd[k]=parseInt(temparr[1]) ;
	esxcrd[k]=parseInt(temparr[2]) ;
	esycrd[k]=parseInt(temparr[3]) ;
	specfeat2[k]=parseInt(temparr[4]) ;
    	if ((esxcrd[k]==0) && (esycrd[k]==0) )
    	{
    		var widtex=ctx.measureText(Narray[k]).width;
    		var widtey=5;
    		var text_offsetx=widtex/2;
    		var text_offsety=widtex/2;
    	}
    	else
    	{
    		var widtex=esxcrd[k]-sxcrd[k];
    		var widtey=esycrd[k]-sycrd[k];
    		var text_offsetx=widtex/2;
    		var text_offsety=widtey/2;
    	}
 	var sxcrd1=sxcrd[k] - offsetx;
    	var sycrd1=sycrd[k] - offsety;
    	
    	//从503中取出显示属性
    	var disp=array406[ specfeat2[k]-1 ];
    	//分析显示属性    	
    	//document.write ("disp:" + disp +  ";<br>");
    	var dsparr=disp.split('/');	//  0/1/2/;
    	var xianduan=dsparr[1];		//  ffff0000/1/;
    	var colo2;			//  255/0/0/0/0/;
    	if (dsparr[2]>1)
    		colo2="red";
    	else
    		colo2="black";
   	//------------------------------------------------------------
   	//在图形上挖出结点空间
   	//画上文本，字符串
   	//document.write("x1:" + sxcrd1 + ",y1:" + sycrd1 + ";text_offsetx:" + text_offsetx + ",text_offsety:" + text_offsety +";<br>");
    	//document.write("Nodestr:" + Narray[k] + ";<br>");
    	var NodeNos;    	
    	//document.write("Narray[k]:" + Narray[k] + ";<br>");
    	if (shuziMode !='1') 
    	{	
	    	if (Narray[k]!='C' ) 
	    	{
	    		ctx.beginPath();  
	    		ctx.fillStyle = "white";
	    		if (Narray[k]=='+' ||Narray[k]=='-')
				ctx.fillRect(sxcrd1-2,sycrd1-2,widtex+2,widtey+4);
			else
				ctx.fillRect(sxcrd1-2,sycrd1-2,widtex+2,widtey+8);
			ctx.closePath(); 
	    		ctx.fill();
			ctx.fillStyle = colo2;
			if (sufang>0.8)
				ctx.font="11px Arial";
			else
				ctx.font="9px Arial";
			if (Narray[k] == '+' || Narray[k] == '—>') 
				ctx.font="14px Arial";
			ctx.fillText(Narray[k],sxcrd1-1,sycrd1+widtey+4.5);
			//ctx.fillText(sycrd[k],sxcrd1,sycrd1+widtey+2.5);
			ctx.fill();
		}
	}
	else
	{
		if (str601 != '')
			NodeNos = array601[k];
		else
			NodeNos = array401[k];
	
    		ctx.beginPath();  
    		ctx.fillStyle = "white";
		ctx.fillRect(sxcrd1-2,sycrd1-2,widtex+2,widtey+8);
		ctx.closePath(); 
    		ctx.fill();
		ctx.fillStyle = colo2;
		if (sufang>0.8)
			ctx.font="11px Arial";
		else
			ctx.font="9px Arial";
		if (NodeNos == '+' || NodeNos == '—>') 
			ctx.font="14px Arial";
		ctx.fillText(NodeNos,sxcrd1-1,sycrd1+widtey+4.5);
		//ctx.fillText(sycrd[k],sxcrd1,sycrd1+widtey+2.5);
		ctx.fill();
	}
    }
    
 
}

